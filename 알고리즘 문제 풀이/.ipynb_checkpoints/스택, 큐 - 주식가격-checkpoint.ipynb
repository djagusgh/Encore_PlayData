    "def solution(p):\n",
    "    answer = [0 for i in range(len(p))]\n",
    "    for i in range(len(p) - 1):\n",
    "        for j in range(i, len(p) - 1):\n",
    "            if p[i] > p[j]:\n",
    "                break;\n",
    "            else:\n",
    "                answer[i] += 1\n",
    "    return answer"
   ]
